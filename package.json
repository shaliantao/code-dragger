{
  "name": "code-dragger",
  "version": "0.0.0",
  "description": "code-dragger",
  "main": "dist/electron/main.js",
  "author": "shashaxian",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build:web": "rimraf ./dist && cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build": "rimraf ./dist && rimraf ./release && cross-env NODE_ENV=production npm run build:web && electron-builder",
    "rebuild": "electron-rebuild -f -w node-pty",
    "postinstall": "npm run rebuild",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean:lib": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm install",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "gen:icon": "esno ./build/generate/icon/index.ts"
  },
  "dependencies": {
    "@babel/generator": "^7.16.0",
    "@babel/parser": "^7.16.3",
    "@babel/template": "^7.16.0",
    "@babel/traverse": "^7.16.3",
    "@babel/types": "^7.16.0",
    "@iconify/iconify": "^2.0.4",
    "@vue/runtime-core": "npm:vue-runtime-core-keepalive-enhance@^3.2.31",
    "@vueuse/core": "^6.3.3",
    "@zxcvbn-ts/core": "^1.0.0-beta.0",
    "ant-design-vue": "^3.2.9",
    "axios": "^0.27.2",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.3",
    "ejs": "^3.1.6",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "intro.js": "^4.2.2",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "log4js": "^6.3.0",
    "minimist": "^1.2.5",
    "monaco-editor": "^0.30.0",
    "ncp": "^2.0.0",
    "node-pty": "^0.10.1",
    "node-schedule": "^2.1.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "pinia": "2.0.0-rc.9",
    "qrcode": "^1.4.4",
    "resize-observer-polyfill": "^1.5.1",
    "sortablejs": "^1.14.0",
    "tar": "^6.1.11",
    "vue": "3.2.31",
    "vue-i18n": "9.1.7",
    "vue-router": "^4.0.11",
    "vue-runtime-core-keepalive-enhance": "^3.2.31",
    "vue-types": "^4.1.0",
    "vuedraggable": "^4.1.0",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@iconify/json": "^1.1.401",
    "@purge-icons/generated": "^0.7.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/babel-generator": "^6.25.4",
    "@types/babel-template": "^6.25.2",
    "@types/babel-types": "^7.0.11",
    "@types/chokidar": "^2.1.3",
    "@types/crypto-js": "^4.0.2",
    "@types/ejs": "^3.1.1",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^8.1.1",
    "@types/intro.js": "^3.0.2",
    "@types/lodash-es": "^4.17.5",
    "@types/ncp": "^2.0.5",
    "@types/node": "^16.9.1",
    "@types/node-schedule": "^2.1.0",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.4.1",
    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.10.7",
    "@types/tar": "^6.1.2",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-vue": "2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.1.8",
    "autoprefixer": "^10.3.4",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.6",
    "esbuild": "^0.14.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "esno": "^0.9.1",
    "http-server": "^13.0.1",
    "husky": "^7.0.2",
    "inquirer": "^8.1.2",
    "is-ci": "^3.0.0",
    "less": "^4.1.1",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "5.5.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-electron": "0.8.4",
    "vite-plugin-html": "^2.1.0",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-style-import": "^1.2.1",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-windicss": "^1.4.2",
    "vue-eslint-parser": "^7.11.0",
    "vue-tsc": "^0.3.0"
  },
  "keywords": [
    "rpa",
    "lowcode",
    "vue3",
    "vite",
    "electron"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
